{
  "name": "cisco-dash",
  "version": "1.0.0",
  "description": "Cisco Dash package.json from the documentation, supplemented with testing support",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp devWatch",
    "build:e2e": "gulp e2e",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start:win": "start-mongod.bat && start-server.bat && npm run dev:win",
    "stop:win": "stop-mongod.bat && stop-server.bat",
    "dev:win": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "deploy:win": "deploy.bat",
    "start:nix": "./start-mongod.sh & ./start-server.sh & npm run dev:nix",
    "stop:nix": "./stop-mongod.sh & ./stop-server.sh",
    "dev:nix": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "deploy:nix": "./deploy.sh",
    "clean:nix": "./clean.sh",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "start mongod && ping 127.0.0.1 -n 6 > null && start node server.js && concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose --fix"
  },
  "keywords": [],
  "author": "Bow Archer [Decoded4620] and William Namen",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.26",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.0",
    "bootstrap": "^4.0.0-alpha.6",
    "bootstrap-grid": "^2.0.1",
    "chart.js": "^2.6.0",
    "core-js": "^2.4.1",
    "crypto": "0.0.3",
    "del": "^3.0.0",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^7.3.0",
    "masonry-layout": "^4.2.0",
    "mongodb": "^2.2.29",
    "mongoose": "^4.10.8",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "forever": "^0.15.3",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-clean": "^0.3.2",
    "gulp-jsonminify": "^1.0.0",
    "gulp-notify": "^3.0.0",
    "gulp-sass": "^3.1.0",
    "gulp-typescript": "^3.1.7",
    "gulp-uglify": "^3.0.0",
    "gulp-uglifycss": "^1.0.8",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "node-sass": "^4.5.3",
    "protractor": "~4.0.14",
    "pump": "^1.0.2",
    "tslint": "^3.15.1",
    "typescript": "^2.2.2"
  },
  "repository": {}
}
